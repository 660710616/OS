class Person:
    def __init__(self, name, age, sex):
        self.name = name
        self.age = age
        self.sex = sex
        self.height = 160.0
        self.weight = 50.0
        self.x = 0

    def walk_ahead(self, distance=0):
        self.x += distance

    def can_drink(self):
        return self.age > 20

    def __str__(self):  #toString()
        return f"{self.name}[{self.age}]"
